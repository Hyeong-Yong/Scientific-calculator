    "# Absorption law\n",
    "import math\n",
    "I_0 = 1\n",
    "alpha= 150 # [cm^-1]\n",
    "x= 300 # [um]\n",
    "x_cm = x*math.pow(10, -4) #[cm]\n",
    "I = I_0 * math.exp(-alpha*x_cm)\n",
    "\n",
    "print(\"absorption percentage : \", I/I_0 *100, \"[%]\")\n",
    "\n"
